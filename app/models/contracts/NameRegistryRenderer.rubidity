pragma :rubidity, "1.0.0"

contract :NameRegistryRenderer, abstract: true do
  function :renderCard, {
    tokenId: :uint256,
    name: :string,
    # # displayName: :string,
    # stickerImages: [:string],
    # stickerPositionXCoords: [:uint256],
    # stickerPositionYCoords: [:uint256],
  }, :public, :view, returns: :string do
    <<~HTML
      <svg xmlns="http://www.w3.org/2000/svg" width="1200" height="630" viewBox="0 0 1200 630">
        #{_pathCss()}
        #{_backgroundPath()}
        #{_logoSvg()}
        <foreignObject x="0" y="0" width="100%" height="100%">
          <style>
          * {
            box-sizing: border-box;
            margin: 0;
            padding: 0;
            border: 0;
            text-rendering: optimizeLegibility;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
            font-family: 'Helvetica Neue', sans-serif;
          }
          
          .outer {
            display: grid;
            grid-template-rows: 1fr auto 1fr;
            aspect-ratio: 1200 / 630;
            padding: 20px;
          }
          
          .name {
            display: flex;
            justify-content: center;
            align-items: center;
            grid-row-start: 2;
            font-size: 100px;
          }
          
          .footer {
            display: flex;
            justify-content: space-between;
            grid-row-start: 3;
            align-items: flex-end;
            font-size: 65px;
          }
          
          .tokenId {
            color: #696969;
            font-family: monospace;
            padding: 18px;
          }
          </style>
          <div class="outer" xmlns="http://www.w3.org/1999/xhtml">
            <div class="name">
              #{name}
            </div>
            <div class="footer">
              <div class="tokenId">
                ##{tokenId.toString()}
              </div>
            </div>
          </div>
        </foreignObject>
      </svg>
    HTML
  end
  
